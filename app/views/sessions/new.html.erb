<%= form_tag (sessions_path) do %>

  <%= label_tag 'email', 'Email' %>
  <%= text_field_tag 'email' %>

  <%= label_tag 'password', 'Password' %>
  <%= password_field_tag 'password' %>

  <%= submit_tag "Sign In"%>
<% end %>

<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.submit %>
<% end %>

